{
    "cluster_name": "mmt_gwb_14.3LTSgpu",
    "spark_version": "14.3.x-scala2.12",
    "aws_attributes": {
        "zone_id": "us-west-2c"
    },
    "node_type_id": "g5.24xlarge",
    "custom_tags": {
        "gwb_nvidia_collab": "True",
        "removeAfter": "2026-01-31"
    },
    "cluster_log_conf": {
        "volumes": {
            "destination": "/Volumes/mmt/genesiswb/scimilarity/logs"
        }
    },
    "autotermination_minutes": 120,
    "enable_elastic_disk": true,
    "single_user_name": "may.merkletan@databricks.com",
    "data_security_mode": "DATA_SECURITY_MODE_AUTO",
    "runtime_engine": "STANDARD",
    "kind": "CLASSIC_PREVIEW",
    "use_ml_runtime": true,
    "is_single_node": false,
    "autoscale": {
        "min_workers": 1,
        "max_workers": 4
    },
    "apply_policy_default_values": false
}